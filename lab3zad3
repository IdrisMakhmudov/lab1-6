#include <iostream>
using namespace std;

int main() {
    float A, B, C, Z;
    cout << "Enter A,B and C: ";
    cin >> A >> B >> C;
    Z=A; A=B; B=C; C=Z;
    cout << "A: " << A << "; B: " << B << "; C: " << C;
    return 0;
    }
//3. Даны переменные A, B, C. Изменить их значения, переместив
//содержимое A в C, C — в B, B — в A, и вывести новые значения переменных
//A, B, C.
